<?xml version="1.0" encoding="UTF-8"?>
<tag-lib>
	<description>标准核心库</description>
	<version>1.0</version>
	<name>tag-smile-core</name>
	<tag>
		<description>配合switch标签使用</description>
		<name>case</name>
		<tag-class>org.smile.tag.impl.CaseTag</tag-class>
	</tag>
	<tag>
		<description>定义一个变量</description>
		<name>declare</name>
		<tag-class>org.smile.tag.impl.DeclareTag</tag-class>
	</tag>
	<tag>
		<description>配合swtich使用</description>
		<name>default</name>
		<tag-class>org.smile.tag.impl.DefaultTag</tag-class>
	</tag>
	<tag>
		<description>配置if标签使用</description>
		<name>elseif</name>
		<tag-class>org.smile.tag.impl.ElseIfTag</tag-class>
	</tag>
	<tag>
		<description>配置if标签使用</description>
		<name>else</name>
		<tag-class>org.smile.tag.impl.ElseTag</tag-class>
	</tag>
	<tag>
		<description>循环标签</description>
		<name>foreach</name>
		<tag-class>org.smile.tag.impl.ForEachTag</tag-class>
	</tag>
	<tag>
        <description>连接标签</description>
        <name>join</name>
        <tag-class>org.smile.tag.impl.JoinTag</tag-class>
    </tag>
	<tag>
		<description>for循环 start end step </description>
		<name>for</name>
		<tag-class>org.smile.tag.impl.ForTag</tag-class>
	</tag>
	<tag>
		<description>获取一个值</description>
		<name>get</name>
		<tag-class>org.smile.tag.impl.GetTag</tag-class>
	</tag>
	<tag>
		<description>if else 的外包标签  字标签为 if 标签  else 标签 </description>
		<name>ifelse</name>
		<tag-class>org.smile.tag.impl.IfElseTag</tag-class>
	</tag>
	<tag>
		<description>if标签</description>
		<name>if</name>
		<tag-class>org.smile.tag.impl.IfTag</tag-class>
	</tag>
	<tag>
		<description>迭代器</description>
		<name>iterator</name>
		<tag-class>org.smile.tag.impl.IteratorTag</tag-class>
	</tag>
	<tag>
		<description>定义一个list 可以子标签内进行迭代 获取等</description>
		<name>list</name>
		<tag-class>org.smile.tag.impl.ListTag</tag-class>
	</tag>
	<tag>
		<description>为list标签添加元素</description>
		<name>listNode</name>
		<tag-class>org.smile.tag.impl.ListNodeTag</tag-class>
	</tag>
	<tag>
		<description>定义一个map</description>
		<name>map</name>
		<tag-class>org.smile.tag.impl.MapTag</tag-class>
	</tag>
	<tag>
		<description>为map标签添加元素</description>
		<name>mapNode</name>
		<tag-class>org.smile.tag.impl.MapNodeTag</tag-class>
	</tag>
	<tag>
		<description>when的子标签 配合then标签 </description>
		<name>other</name>
		<tag-class>org.smile.tag.impl.OtherTag</tag-class>
	</tag>
	<tag>
		<description>为url添加多个参数  json 或  map </description>
		<name>params</name>
		<tag-class>org.smile.tag.impl.ParamsTag</tag-class>
	</tag>
	<tag>
		<description>为url添加参数</description>
		<name>param</name>
		<tag-class>org.smile.tag.impl.ParamTag</tag-class>
	</tag>
	<tag>
		<description>移除上下文中的内容</description>
		<name>remove</name>
		<tag-class>org.smile.tag.impl.RemoveTag</tag-class>
	</tag>
	<tag>
		<description>在上下文中设置内容</description>
		<name>set</name>
		<tag-class>org.smile.tag.impl.SetTag</tag-class>
	</tag>
	<tag>
		<description>从国际化中获取文本设置到上下文中</description>
		<name>setText</name>
		<tag-class>org.smile.tag.impl.SetTextTag</tag-class>
	</tag>
	<tag>
		<description>switch标签 ,子标签为case default </description>
		<name>switch</name>
		<tag-class>org.smile.tag.impl.SwitchTag</tag-class>
	</tag>
	<tag>
		<description>显示一个文本</description>
		<name>text</name>
		<tag-class>org.smile.tag.impl.TextTag</tag-class>
	</tag>
	<tag>
		<description>when的子标签</description>
		<name>then</name>
		<tag-class>org.smile.tag.impl.ThenTag</tag-class>
	</tag>
	<tag>
		<description>移除上下文中的内容</description>
		<name>unset</name>
		<tag-class>org.smile.tag.impl.UnsetTag</tag-class>
	</tag>
	<tag>
		<description>定义一个url对象</description>
		<name>url</name>
		<tag-class>org.smile.tag.impl.UrlTag</tag-class>
	</tag>
	<tag>
		<description>when标签  子标签为 then other</description>
		<name>when</name>
		<tag-class>org.smile.tag.impl.WhenTag</tag-class>
	</tag>
</tag-lib>
